<!-- Loop over each object in the projects array -->
{{#each blogs as |blog| }}
<div>
  <div>
    <h2>
      <a href="/blog/{{blog.id}}">{{blog.title}}{{blog.date_created}}</a>
    </h2>
    <p>Created by {{blog.user.name}} on {{format_date blog.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{blog.content}}
    </p>
  </div>
</div>
{{/each}}
