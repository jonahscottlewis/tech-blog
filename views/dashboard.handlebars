{{#if blog.length}}
<div>
    <h1>Dashboard</h1>
    {{#each blogs as |blog|}}
    <div class="card">
        <h3 class="blog-title">{{title}}</h3>
        <p class="content">{{content}}</p>
    </div>
    {{/each}}
</div>
{{/if}}

<button type="submit" class="btn btn-primary">+ a post</button>
<script src="js/newBlog.js"></script>